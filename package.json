{
    "name": "normalize-css",
    "version": "1.0.0",
    "description": "A modern alternative to CSS resets",
    "repository": "git@github.com:KunalTanwar/normalize-css.git",
    "author": "KunalTanwar <kunaltanwar2@gmail.com>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "compile-normalize": "sass scss/normalize.scss --no-source-map -s compressed css/normalize.min.css",
        "prefix-normalize": "postcss css/normalize.min.css -u autoprefixer --no-map -r",
        "compile-normalize-inter": "sass scss/normalize.inter.scss --no-source-map -s compressed css/normalize.inter.min.css",
        "prefix-normalize-inter": "postcss css/normalize.inter.min.css -u autoprefixer --no-map -r"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "postcss": "^8.4.5",
        "postcss-cli": "^9.1.0",
        "sass": "^1.45.0"
    }
}
